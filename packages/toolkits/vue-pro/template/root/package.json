    "unplugin-vue-components": "^0.17.21",
    "vite": "^2.6.4",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-style-import": "1.4.1",
    "vite-svg-loader": "^3.1.0",
    "vue-tsc": "^0.30.5",
