    "@vueuse/core": "^7.3.0",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "nprogress": "^1.0.0-1",
    "pinia": "2.0.13",
    "query-string": "^7.0.1",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.17",
    "vue-router": "^4.0.14"
