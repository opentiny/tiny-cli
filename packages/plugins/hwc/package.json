{
  "name": "@opentiny/tiny-plugin-hwc",
  "private": false,
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "tinyOption": {
    "type": "plugin",
    "update": true,
    "chName": ""
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tiny",
    "tiny-plugin"
  ],
  "repository": "https://github.com/opentiny/tiny-cli",
  "scripts": {
    "debug": "node debug.js",
    "describe": "npm-scripts-info",
    "build": "run-s build:*",
    "build:main": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint --fix --project .",
    "test": "run-s build test:*",
    "test:lint": "tslint --project . && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "nyc --silent ava",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" ",
    "cov": "run-s build test:unit cov:html && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "doc:html": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --exclude **/*.spec.ts --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "clean": "trash build test",
    "prepublishOnly": "run-s fix build"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepublishOnly": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@actions/tool-cache": "^2.0.1",
    "@huaweicloud/huaweicloud-sdk-core": "^3.0.52-rc",
    "@huaweicloud/huaweicloud-sdk-iam": "^3.0.52-rc",
    "@opentiny/cli-devkit": "^1.0.0-alpha.0",
    "archiver": "^5.3.1",
    "chalk": "^2.4.2",
    "cli-table3": "^0.6.3",
    "esdk-obs-nodejs": "^3.21.6",
    "inquirer": "^8.0.2",
    "lodash": "^4.17.21",
    "parse-gitignore": "^2.0.0",
    "proxy-agent": "^5.0.0",
    "shelljs": "^0.8.5",
    "string-random": "^0.1.3"
  },
  "devDependencies": {
    "@bitjson/npm-scripts-info": "1.0.0",
    "@types/archiver": "^5.3.1",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^8.2.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "12.6.8",
    "@types/parse-gitignore": "^1.0.0",
    "@types/shelljs": "^0.8.11",
    "ava": "2.2.0",
    "codecov": "3.5.0",
    "cz-conventional-changelog": "2.1.0",
    "gh-pages": "2.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "2.7.1",
    "trash-cli": "3.0.0",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "^4.7.4"
  },
  "ava": {
    "failFast": true,
    "files": [
      "dist/**/*.spec.js"
    ],
    "sources": [
      "dist/**/*.js"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "changeLog": [
    {
      "version": "1.0.0",
      "log": [
        "feat: 初始化项目"
      ]
    }
  ]
}
